],
  
  "background": {
    "service_worker": "background-batch.js"
  },
  
"web_accessible_resources": [
  {
    "resources": [
      "directories/master-directory-list.json"
    ],
    "matches": [
      "https://directorybolt.com/*"
    ]
  }
],
  
  "content_security_policy": {
  "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://directorybolt.com https://sheets.googleapis.com;"
},
  
  "minimum_chrome_version": "88",
  
  
  "externally_connectable": {
    "matches": ["https://directorybolt.com/*"]
  },
  
  "commands": {
    "quick_fill": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Command+Shift+F"
      },
      "description": "Quick fill current form"
    }
  }
}
