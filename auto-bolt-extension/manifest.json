{
  "manifest_version": 3,
  "name": "DirectoryBolt Extension",
  "version": "3.0.2",
  "description": "DirectoryBolt secure customer extension for automated directory submissions via secure proxy.",
  "author": "DirectoryBolt Team",
  "homepage_url": "https://directorybolt.com",
  
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "notifications"
  ],
  
  "host_permissions": [
    "https://directorybolt.com/*"
  ],
  
  "action": {
    "default_popup": "customer-popup.html",
    "default_title": "DirectoryBolt - Directory Automation",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  
