{
  "name": "directorybolt",
  "version": "2.0.1-emergency-fix",
  "private": true,
  "scripts": {
    "build": "node scripts/verify-build.js && cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "build:fast": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "build:minimal": "cross-env NEXT_CONFIG_FILE=\"next.config.minimal.js\" next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:netlify": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "analyze": "npm run build:analyze",
    "type-check": "tsc --noEmit",
    "postbuild": "next-sitemap",
    "test:ai": "node scripts/test-ai-services.js",
    "test:puppeteer": "node scripts/test-puppeteer-setup.js",
    "test:phase2": "node scripts/test-phase2.js",
    "test:phase2-ux": "node scripts/test-phase2-ux.js",
    "test:seo": "node scripts/seo-validation-test.js",
    "security-test": "node scripts/security-validation-test.js",
    "security-dev": "npm run dev & sleep 5 && npm run security-test && pkill -f 'next dev'",
    "review": "node scripts/quick-code-review.js",
    "review:enhanced": "node scripts/enhanced-code-review-system.js",
    "review:all": "node scripts/enhanced-code-review-system.js --all",
    "review:pre-commit": "node scripts/quick-code-review.js && echo '✅ Code review passed'",
    "test:comprehensive": "node tests/comprehensive-ai-test-runner.js",
    "test:ai-accuracy": "jest tests/ai/ai-accuracy-validation.test.js",
    "test:performance": "jest tests/performance/load-testing.test.js",
    "test:cost-optimization": "jest tests/ai/cost-optimization.test.js",
    "test:e2e": "playwright test",
    "test:e2e-headed": "playwright test --headed",
    "test:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:pricing-tiers": "playwright test tests/e2e/pricing-tier-validation.spec.ts",
    "test:conversion-funnel": "playwright test tests/e2e/upgrade-conversion-funnel.spec.ts",
    "test:user-journey": "playwright test tests/e2e/complete-user-journey.spec.ts",
    "test:ci": "npm run test:comprehensive",
    "test:ai-integration": "node test-ai-integration.js",
    "test:stripe-integration": "node test-stripe-integration.js",
    "test:e2e-integration": "node test-e2e-integration.js",
    "test:protocol": "node run-comprehensive-tests.js",
    "test:quick": "node quick-test-validation.js",
    "test:final": "node run-final-test.js",
    "test:enterprise-qa": "playwright test tests/advanced-qa/enterprise-qa-orchestrator.spec.ts",
    "test:enterprise-qa-headed": "playwright test tests/advanced-qa/enterprise-qa-orchestrator.spec.ts --headed",
    "test:ai-powered": "playwright test tests/advanced-qa/enterprise-qa-orchestrator.spec.ts --grep=\"AI-Powered\"",
    "test:accessibility-compliance": "playwright test tests/advanced-qa/enterprise-qa-orchestrator.spec.ts --grep=\"Accessibility Compliance\"",
    "test:performance-rum": "playwright test tests/advanced-qa/enterprise-qa-orchestrator.spec.ts --grep=\"Performance Standards\"",
    "test:quality-gates": "playwright test tests/advanced-qa/enterprise-qa-orchestrator.spec.ts --grep=\"Quality Gates\"",
    "test:continuous-pipeline": "playwright test tests/advanced-qa/enterprise-qa-orchestrator.spec.ts --grep=\"Continuous Testing Pipeline\"",
    "test:premium-saas-comparison": "playwright test tests/advanced-qa/enterprise-qa-orchestrator.spec.ts --grep=\"Premium SaaS QA Methodology\"",
    "qa:full-enterprise": "npm run test:enterprise-qa && echo '✅ Enterprise QA validation completed'",
    "qa:deployment-ready": "npm run test:quality-gates && npm run test:accessibility-compliance && echo '✅ Deployment readiness validated'",
    "validate:guides": "node scripts/validate-json-guides.js",
    "prebuild": "npm run validate:guides",
    "debug:env": "node scripts/advanced-env-debug.js",
    "debug:env-production": "cross-env NODE_ENV=production node scripts/advanced-env-debug.js",
    "optimize:build": "node scripts/advanced-build-optimizer.js",
    "optimize:pre-build": "npm run optimize:build && npm run debug:env",
    "build:optimized": "npm run optimize:pre-build && npm run build",
    "test:production": "node scripts/production-deployment-test.js",
    "deploy:ready": "npm run test:production && echo '✅ Deployment ready for production'"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
