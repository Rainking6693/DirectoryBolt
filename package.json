{
  "name": "directorybolt",
  "version": "1.0.0",
  "private": true,
  "description": "DirectoryBolt.com - High-performance Next.js application with enterprise-grade infrastructure",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "start:production": "cross-env NODE_ENV=production next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "seed:directories": "node scripts/seed-directories.js --action=seed",
    "seed:validate": "node scripts/seed-directories.js --action=validate --dry-run",
    "seed:reset": "node scripts/seed-directories.js --action=reset --drop-existing",
    "seed:stats": "node scripts/seed-directories.js --action=stats",
    "db:seed": "npm run seed:directories",
    "db:reset": "npm run seed:reset",
    "db:status": "npm run seed:stats",
    "seo:test": "node tests/seo-comprehensive-test-runner.js",
    "seo:test:quick": "node tests/seo-automated-validators.js",
    "seo:check": "node tests/quick-seo-check.js",
    "seo:test:external": "RUN_EXTERNAL=true node tests/seo-comprehensive-test-runner.js",
    "seo:test:production": "cross-env NODE_ENV=production RUN_EXTERNAL=true node tests/seo-comprehensive-test-runner.js",
    "seo:validate:robots": "node -e \"const SEOValidators = require('./tests/seo-automated-validators'); const v = new SEOValidators(); v.validateRobotsTxt().then(r => console.log(JSON.stringify(r, null, 2)));\"",
    "seo:validate:sitemap": "node -e \"const SEOValidators = require('./tests/seo-automated-validators'); const v = new SEOValidators(); v.validateSitemapXml().then(r => console.log(JSON.stringify(r, null, 2)));\"",
    "seo:validate:meta": "node -e \"const SEOValidators = require('./tests/seo-automated-validators'); const v = new SEOValidators(); v.validateMetaTags().then(r => console.log(JSON.stringify(r, null, 2)));\"",
    "seo:validate:structured": "node -e \"const SEOValidators = require('./tests/seo-automated-validators'); const v = new SEOValidators(); v.validateStructuredData().then(r => console.log(JSON.stringify(r, null, 2)));\"",
    "seo:test:ci": "cross-env NODE_ENV=production VERBOSE=false node tests/seo-comprehensive-test-runner.js",
    "stripe:test": "node scripts/test-stripe-integration.js",
    "stripe:test:local": "TEST_BASE_URL=http://localhost:3000 node scripts/test-stripe-integration.js",
    "deploy": "vercel --prod",
    "deploy:production": "node scripts/deploy-production.js",
    "deploy:monitor": "node scripts/production-monitor.js",
    "docker:build": "docker build -t directorybolt .",
    "docker:run": "docker run -p 3000:3000 directorybolt",
    "docker:compose": "docker-compose up -d",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "test:integration": "npm run build && npm run start:production & sleep 10 && npm run health-check && pkill -f 'next start'",
    "optimize": "npm run clean && npm run build:production && npm run build:analyze"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.5.2",
    "@supabase/supabase-js": "^2.45.4",
    "@types/node": "^20.16.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "critters": "^0.0.23",
    "isomorphic-dompurify": "^2.15.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.2.15",
    "openai": "^5.16.0",
    "rate-limiter-flexible": "^5.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stripe": "^16.12.0",
    "typescript": "^5.6.2",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/bcryptjs": "^2.4.6",
    "@types/cheerio": "^0.22.35",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/validator": "^13.12.2",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.15",
    "postcss": "^8.4.47",
    "puppeteer": "^24.17.0",
    "sharp": "^0.32.6",
    "tailwindcss": "^3.4.13"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "tailwindcss",
    "directory",
    "business"
  ],
  "author": "DirectoryBolt Team",
  "license": "MIT"
}
