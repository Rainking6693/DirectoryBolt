    "test:enterprise-qa-headed": "playwright test tests/advanced-qa/enterprise-qa-orchestrator.spec.ts --headed",
    "test:ai-powered": "playwright test tests/advanced-qa/enterprise-qa-orchestrator.spec.ts --grep=\"AI-Powered\"",
    "test:accessibility-compliance": "playwright test tests/advanced-qa/enterprise-qa-orchestrator.spec.ts --grep=\"Accessibility Compliance\"",
    "test:performance-rum": "playwright test tests/advanced-qa/enterprise-qa-orchestrator.spec.ts --grep=\"Performance Standards\"",
