    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "framer-motion": "^12.23.12",
    "google-auth-library": "^10.3.0",
    "google-spreadsheet": "^5.0.2",
    "googleapis": "^139.0.0",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.5",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "lucide-react": "^0.542.0",
    "metascraper": "^5.34.0",
    "micro": "^10.0.1",
    "next": "^14.2.32",
    "next-sitemap": "^4.2.3",
    "node-fetch": "^2.7.0",
    "node-html-parser": "^6.1.0",
    "nodemailer": "^7.0.6",
    "openai": "^4.0.0",
    "pg": "^8.16.3",
    "postcss": " ^8.4.31 ",
    "puppeteer": "^24.20.0",
    "puppeteer-core": "^24.19.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "stripe": "^13.8.0",
    "tailwindcss": " ^3.4.17 ",
    "typescript": "5.2.2",
    "uuid": "^9.0.1",
    "valibot": "^1.1.0",
    "workbox-webpack-plugin": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@netlify/plugin-nextjs": "^5.13.0",
    "@next/bundle-analyzer": "^14.0.0",
    "@next/react-refresh-utils": "^15.5.3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/archiver": "^6.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cheerio": "^0.22.31",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/nodemailer": "^7.0.1",
    "@types/uuid": "^9.0.7",
    "adm-zip": "^0.5.10",
    "archiver": "^6.0.1",
    "cli-progress": "^3.12.0",
    "cross-env": "^7.0.3",
    "eslint": "8.51.0",
    "eslint-config-next": "14.0.0",
    "jest": "^30.1.3",
