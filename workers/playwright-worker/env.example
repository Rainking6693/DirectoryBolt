# AI-Enhanced Playwright Worker Environment Variables
# Copy this to .env and fill in your actual values

# Basic Configuration
NODE_ENV=development
PORT=3000
POLL_INTERVAL=5000
WORKER_ID=local-ai-worker-1

# API Configuration (Production URLs)
AUTOBOLT_API_KEY=your_autobolt_api_key_here
AUTOBOLT_API_BASE=https://directorybolt.com
NETLIFY_FUNCTIONS_URL=https://directorybolt.com
WORKER_AUTH_TOKEN=your_worker_auth_token_here

# Playwright Configuration
PLAYWRIGHT_HEADLESS=false
PLAYWRIGHT_TIMEOUT=30000
HEADLESS=false

# AI Services - REQUIRED for full functionality
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# CAPTCHA Service
TWO_CAPTCHA_API_KEY=your_2captcha_api_key_here

# Database Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Logging
LOG_LEVEL=debug
